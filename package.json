{
  "name": "izumo-taisha-tokyo-bunshi",
  "version": "1.0.0",
  "scripts": {
    "build": "node build.js",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=izumo-taisha-tokyo-bunshi --branch=main",
    "sync": "node sync-news.js && npm run build && npm run deploy",
    "db:migrate:local": "wrangler d1 migrations apply izumo-cms --local",
    "db:migrate:prod": "wrangler d1 migrations apply izumo-cms"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250705.0",
    "esbuild": "^0.19.0",
    "wrangler": "^3.78.0"
  }
}
